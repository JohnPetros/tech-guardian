<div class="checkbox-list" data-checkbox-list>
  <span class="checkbox-list-overlay" data-checkbox-list-overlay></span>
  <button class="checkbox-list-button" type="button">
    <strong class="title">Patrim√¥nios</strong>
    <i class="icon ph-bold ph-caret-down"></i>
  </button>
  <ul class="checkboxes">
    <% items.forEach(item => { %>
      <li>
        <label for="<%= item.id %>" class="checkbox" tabindex="0">
          <input 
            type="checkbox" 
            name="<%= name %>[]" 
            id="<%= item.id %>" 
            value="<%= item.id %>" 
            tabindex="-1"
            <% if (checkedItemsIds && checkedItemsIds.includes(item.id)) { %>
              checked
            <% } %>
          />
          <span><%= item.value %></span>
        </label>
      </li>
    <% }) %>
   
    <button type="submit" class="filter-button">Filtrar</button>
  </ul>
</div>
